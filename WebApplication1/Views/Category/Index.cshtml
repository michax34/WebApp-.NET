@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <th>
            Category Name
        </th>
        <th>
            Display Order
        </th>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="30%">
                    @obj.DisplayyOrder
                </td>
            </tr>
        }
    </tbody>
</table>